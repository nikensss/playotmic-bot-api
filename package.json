{
  "name": "body-metrics-nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "db": "docker compose up dev-db -d",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --check",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prebuild": "rimraf dist",
    "prepare": "husky install",
    "prettier": "prettier \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start -e \"ts-node -r dotenv/config -r newrelic\"",
    "start:debug": "nest start --debug --watch -e \"ts-node -r dotenv/config -r newrelic\"",
    "start:dev": "nest start --watch -e \"ts-node -r dotenv/config -r newrelic\"",
    "start:prod": "node -r dotenv/config -r newrelic dist/main",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@nestjs/common": "8.0.0",
    "@nestjs/config": "2.1.0",
    "@nestjs/core": "8.0.7",
    "@nestjs/platform-fastify": "8.4.6",
    "@nestjs/swagger": "5.2.1",
    "@prisma/client": "3.15.1",
    "cls-proxify": "2.0.0",
    "dotenv": "16.0.1",
    "fastify": "3.29.0",
    "fastify-swagger": "5.2.0",
    "joi": "17.6.0",
    "newrelic": "8.14.0",
    "pino": "8.0.0",
    "pino-pretty": "8.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.2.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@nestjs/cli": "8.0.0",
    "@nestjs/schematics": "8.0.0",
    "@nestjs/testing": "8.0.0",
    "@types/express": "4.17.13",
    "@types/jest": "27.5.0",
    "@types/node": "16.0.0",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "8.0.1",
    "jest": "28.0.3",
    "prettier": "2.3.2",
    "prisma": "3.15.1",
    "source-map-support": "0.5.20",
    "supertest": "6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "9.2.3",
    "ts-node": "10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
